<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="mainVBox" prefHeight="700" prefWidth="1000" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Tomasulo.MainController">
    <VBox id="menuHBox">
        <ProgressBar id="clock-bar" fx:id="clockBar" prefHeight="2.0" prefWidth="1000" progress="0" />
        <HBox>
            <MenuBar prefWidth="1000">
                <menus>
                    <Menu mnemonicParsing="false" text="文件">
                        <items>
                            <MenuItem mnemonicParsing="false" onAction="#close" text="关闭" />
                        </items>
                    </Menu>
                    <Menu fx:id="clockLabel" disable="true" mnemonicParsing="false" style="-fx-opacity: 1 !important;" text="未运行" />
                </menus>
            </MenuBar>
            <Button minWidth="80.0" mnemonicParsing="false" onAction="#start" text="重启" />
            <Button minWidth="80.0" mnemonicParsing="false" onAction="#step" text="下一步" />
            <Button fx:id="playButton" minWidth="80.0" mnemonicParsing="false"
                    onAction="#play"
                    text="播放" />
        </HBox>
    </VBox>
   <HBox fx:id="mainHBox" alignment="TOP_RIGHT" prefHeight="676.0" spacing="10.0">
      <children>
         <VBox prefHeight="676.0" prefWidth="326.0" spacing="10.0">
             <children>
                 <TableView id="ins-table" fx:id="instructionTable" editable="true" prefHeight="1304.0" prefWidth="326.0">
                     <columns>
                         <TableColumn id="ins-table-name" text="指令" />
                         <TableColumn id="ins-table-dest" text="目的" />
                         <TableColumn id="ins-table-source" text="源" />
                         <TableColumn id="ins-table-emit" text="发射" />
                         <TableColumn id="ins-table-done" text="执行" />
                         <TableColumn id="ins-table-back" text="写回" />
                     </columns>
                     <columnResizePolicy>
                         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                 </TableView>
             </children>
         </VBox>
         <VBox prefHeight="676.0" prefWidth="326.0" spacing="10.0">
             <children>
                 <TableView id="add-table" fx:id="addStationTable" editable="false" prefHeight="169.0" prefWidth="326.0">
                     <columns>
                         <TableColumn text="序号" />
                         <TableColumn text="指令" />
                         <TableColumn text="忙" />
                         <TableColumn text="剩余" />
                         <TableColumn text="总周期" />
                         <TableColumn text="V1" />
                         <TableColumn text="V2" />
                     </columns>
                     <columnResizePolicy>
                         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                 </TableView>
                 <TableView id="mul-table" fx:id="mulStationTable" editable="false" prefHeight="169.0" prefWidth="326.0">
                     <columns>
                         <TableColumn text="序号" />
                         <TableColumn text="指令" />
                         <TableColumn text="忙" />
                         <TableColumn text="剩余" />
                         <TableColumn text="总周期" />
                         <TableColumn text="V1" />
                         <TableColumn text="V2" />
                     </columns>
                     <columnResizePolicy>
                         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                 </TableView>
                 <TableView id="load-table" fx:id="loadStationTable" editable="true" prefHeight="169.0" prefWidth="326.0">
                     <columns>
                         <TableColumn text="序号" />
                         <TableColumn id="load-table-busy" text="忙" />
                         <TableColumn text="剩余" />
                         <TableColumn text="总周期" />
                         <TableColumn id="load-table-addr" text="地址" />
                         <TableColumn id="load-table-cache" text="Cache" />
                     </columns>
                     <columnResizePolicy>
                         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                 </TableView>
                 <TableView id="store-table" fx:id="storeStationTable" editable="true" prefHeight="169.0" prefWidth="326.0">
                     <columns>
                         <TableColumn text="序号" />
                         <TableColumn id="store-table-busy" text="忙" />
                         <TableColumn text="剩余" />
                         <TableColumn text="总周期" />
                         <TableColumn id="store-table-addr" text="地址" />
                         <TableColumn id="store-table-qi" text="源" />
                     </columns>
                     <columnResizePolicy>
                         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                 </TableView>
             </children>
         </VBox>
         <VBox prefHeight="676.0" prefWidth="326.0" spacing="10.0">
            <children>
               <TableView id="registers-table" fx:id="registersTable" editable="false" prefHeight="338.0" prefWidth="326.0">
                  <columns>
                     <TableColumn id="registersTableReg" text="寄存器" />
                      <TableColumn id="registersTableSta" text="保留站" />
                     <TableColumn id="registersTableVal" text="值" />
                  </columns>
                  <columnResizePolicy>
                     <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
               </TableView>
               <TableView id="memory-table" fx:id="memoryTable" editable="true" prefHeight="338.0" prefWidth="326.0">
                  <columns>
                     <TableColumn id="registers-table-addr" text="地址" />
                     <TableColumn id="registers-table-val" text="值" />
                  </columns>
                  <columnResizePolicy>
                     <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
               </TableView>
            </children>
         </VBox>
      </children>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </HBox>
</VBox>
